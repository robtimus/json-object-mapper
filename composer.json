{
    "name": "robtimus/json-object-mapper",
    "type": "library",
    "description": "A library to map objects to and from JSON",
    "homepage": "https://robtimus.github.io/json-object-mapper",
    "keywords": ["JSON"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rob Spoor",
            "email": "robtimus@users.noreply.github.com"
        }
    ],
    "support": {
        "issues": "https://github.com/robtimus/json-object-mapper/issues"
    },
    "require": {
        "php": ">=5.3.2",
        "doctrine/annotations": ">=1.2.7",
        "doctrine/cache": ">=1.5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": {
            "Robtimus\\JSON\\Mapper\\": ["src/Robtimus/JSON/Mapper"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robtimus\\JSON\\Mapper\\": ["tests/Robtimus/JSON/Mapper"]
        }
    }
}
